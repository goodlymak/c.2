#include <stdio.h>

void main() 
    {
        int a;
        int b;
        int c;
        printf("\nenter value of a:= ");
        scanf("%d",&a);
        printf("\nenter value of b:= ");
        scanf("%d",&b);
        c=a;
        a=b;
        b=c;
        printf("enter value after swapping a=%d and \n b=%d",a,b);
    
    }
    
