#include <stdio.h>

int main()
{
    double pi=3.1415926535897932384626433832795;
    float r;
    float volume;
    float area;
    printf("enter radius of spheare= ");
    scanf("%f",&r);
     area=4*pi*r*r;
     volume=(4/3.0)*pi*(r*r*r);
    printf("area of sphare=%f ", area);
    printf("\n");
    printf("volume of sphare=%f",volume);
    return 0;
}
